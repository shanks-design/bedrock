{
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps --prefer-npm",
  "framework": "nextjs",
  "packageManager": "npm",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "GROQ_API_KEY": "@groq_api_key",
    "NEYNAR_API_KEY": "@neynar_api_key"
  },
  "build": {
    "env": {
      "NPM_CONFIG_PACKAGE_MANAGER": "npm"
    }
  }
}
